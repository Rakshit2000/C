#include<stdio.h>


int main()
{
    int arr[200],n,i,sum=0;
    printf("Total elements in an array: ");
    scanf("%d",&n);
    
    printf("Enter the elements in the array: \n");
    for(i=0;i<n;i++)
    {
        printf("Element [%d]: ",i);
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        sum+=arr[i];
    }
    printf("The sum of array elements is : %d",sum);
    

    return 0;
}
