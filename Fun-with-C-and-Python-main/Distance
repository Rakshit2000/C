//C:
#include<stdio.h>
#include<math.h>
int main()
{
    int x1,x2,y1,y2;
    scanf("%d %d %d %d",&x1,&y1,&x2,&y2);
    double distance;
    distance = sqrt(pow(x2-x1,2)+pow(y2-y1,2));
    printf("%lf",distance);
    
    return 0;
}
//Python:
import math
x1=3
y1=4
x2=7
y2=7
distance = math.sqrt(pow(x2-x1,2)+pow(y2-y1,2))
print(distance)
    
