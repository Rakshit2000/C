//C :

#include<stdio.h>
int main()
{
    int a,b,c,d;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    
    if(a<b && a<c && a<d){
        printf("%d",a);
    } 
    else if(b<a && b<c && b<d)
    {
        printf("%d",b);
    }
    else if(c<a && c<b && c<d)
    {
        printf("%d",c);
    }
    else
    {
        printf("%d",d);
    }
   
    return 0;
}

//Python :
a,b,c,d = map(int,input().split())

def minoffour():
    if a<b and a<c and a<d:
        return a
    elif b<a and b<c and b<d:
        return b
    elif c<a and c<b and c<d:
        return c
    else:
        return d
        

print(minoffour())        
        





