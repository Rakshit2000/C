// C:
// Using Recursion
#include<stdio.h>
void printDigit(int n)
{
    int r;
    
    if(n==0)
    {
        return 0;
    }
    r=n%10;
    printDigit(n/10);
    printf("%d",r);
}


int main()
{
    int n;
    scanf("%d",&n);
    printDigit(n);
    
    return 0;
}

//Python:

n =input("")
int(n[0:])
print(n)
