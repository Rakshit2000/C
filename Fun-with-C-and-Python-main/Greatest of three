//C :
#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    
    if(a>b && a>c)
    {
        printf("%d",a);
    }
    else if(b>a && b>c)
    {
        printf("%d",b);
    }
    else
    {
        printf("%d",c);
    }
    return 0;
}


//Python
a = int(input())
b = int(input())
c = int(input())
def greatestOfthree():

    if a>b and a>c:
        return a
    if b>a and b>c:
        return b
    else:
        return c


print(greatestOfthree())
