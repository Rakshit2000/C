// C:


#include <stdio.h>

int main()
{
     int i,j,rows,cols;
    printf("Enter number of rows: ");
    scanf("%d",&rows);
    printf("\nEnter number of coloumns: ");
    scanf("%d",&cols);
    
    for(i=1;i<=rows;i++)
    {
        for(j=1;j<=cols;j++)
        {
            if(i==1||i==rows||j==1||j==rows)
            {
                printf("1");
            }
            else
            {
                printf("0");
            }
        }
        printf("\n");
    }
    return 0;
}


//Python:
rows,cols = map(int, input().split())
for i in range(1,rows+1):
    for j in range(1,cols+1):
        if(i==1 or i==rows or j==1 or j==cols):
            print("1")
        else:
            print("0")
            
