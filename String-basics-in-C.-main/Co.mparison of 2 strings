#include <stdio.h>
#include<string.h>
int main()
{
    char str1[50],str2[50];
    printf("Enter string 1: ");
    gets(str1);
    printf("Enter string 2: ");
    gets(str2);
    int i =strcmp(str1,str2);
    if(i==0)
        printf("Same string");
    else if(i<0) 
        printf("String 1 is smaller than String 2");
    else if(i>0) 
        printf("String 2 is larger than String 1");    
  
    return 0;
}
