#include <stdio.h>
#include<string.h>
int main()
{
    char str[50];
    int i,count1=0,count2=0;
    printf("Enter the string: ");
    gets(str);

    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]=='a' || str[i]=='e' ||  str[i]=='i' ||  str[i]=='o' ||  str[i]=='u')
            count1++;
        else
            count2++;
    }
    printf("Vowel count = %d\n",count1);
    printf("Consonant count = %d\n",count2);


   
    return 0;
}
